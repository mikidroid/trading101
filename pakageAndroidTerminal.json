{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "export NODE_OPTIONS=--openssl-legacy-provider && mix",
        "watch": "export NODE_OPTIONS=--openssl-legacy-provider && mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "export NODE_OPTIONS=--openssl-legacy-provider && mix --production",
        "postinstall": "npm run prod"
    },
    "engines": {
        "npm": ">=7.x",
        "node": ">=14.x"
    },
    "devDependencies": {
        "@mdi/font": "^5.9.55",
